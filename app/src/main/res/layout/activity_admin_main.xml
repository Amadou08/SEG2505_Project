<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminScreens.AdminMainActivity">

    <Button
        android:id="@+id/btn_license_service"
        android:layout_width="match_parent"
        android:layout_height="55dp"
      android:layout_marginTop="100dp"
        android:onClick="btnLicenseClicked"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/view_bg"
        android:text="license service"
        android:textColor="@color/white"
        app:backgroundTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/btn_health_card_service"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:onClick="btnHealthCardClicked"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/view_bg"
        android:text="Health card service"
        android:textColor="@color/white"
        app:backgroundTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_license_service"

        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/btn_photo_id_service"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:onClick="btnPhotoIdClicked"
        android:layout_marginRight="20dp"
        android:background="@drawable/view_bg"
        android:text="Photo Id service"
        android:textColor="@color/white"
        app:backgroundTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_health_card_service"
        tools:ignore="HardcodedText" />
    <Button
        android:id="@+id/btn_customers"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/view_bg"
        android:text="Customers"
        android:onClick="viewCustomerRecord"
        android:textColor="@color/white"
        app:backgroundTint="@color/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_photo_id_service"
        tools:ignore="HardcodedText" />
</androidx.constraintlayout.widget.ConstraintLayout>